{
  "name": "israeli-bank-actual-importer",
  "version": "1.12.0",
  "description": "Import transactions from Israeli banks into Actual Budget",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run build && npm test",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated"
  },
  "dependencies": {
    "@actual-app/api": "^26.2.0",
    "cron-parser": "^4.9.0",
    "israeli-bank-scrapers": "^6.7.1"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "author": "Israeli Bank Importer Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergienko4/israeli-bank-scrapers-to-actual-budget.git"
  },
  "keywords": [
    "actual-budget",
    "israeli-banks",
    "banking",
    "automation",
    "finance",
    "budgeting"
  ]
}
